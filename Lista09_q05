import random
import sys

try:
    m = int(input('Digite o número de linhas da lista A: '))
    n = int(input('Digite o número de colunas da lista A: '))
    m2 = int(input('Digite o número de elementos da lista V: '))

    lista_a = []
    lista_a_coluna = []
    lista_v = []

    for k in range (0,m):
        lista_a.append(lista_a_coluna)
        for i in range (0,n):
            lista_a_coluna.append(random.randrange(1,11))
        

    print (lista_a_coluna)
    print (lista_a)
    print (lista_v)

except ValueError:
    print ('Digite um valor válido')
